                %li.leaf.js-topic
                  %a{:href => "/getting-started/hive.html"} Hive
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/hive/mean-calculation-hive.html"} Average Energy Consumption
                    %li.leaf.js-subtopic
