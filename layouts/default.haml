                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/hawq/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/getting-started/hawq/exercise2.html"} Exercise 2
                %li.leaf.js-topic
                  %a{:href => "/getting-started/data-loader.html"} DataLoader
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/data-loader/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/getting-started/data-loader/exercise2.html"} Exercise 2
                %li.leaf.js-topic
                  %a{:href => "/getting-started/spring-data-hadoop.html"} Spring Data Hadoop
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/spring-data-hadoop/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/getting-started/spring-data-hadoop/exercise2.html"} Exercise 2
                %li.leaf.js-topic
                  %a{:href => "/getting-started/sqoop.html"} Sqoop
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/sqoop/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/getting-started/sqoop/exercise2.html"} Exercise 2
            %li.last.expanded.js-maintopic
              %a{:href => "#"} Deep Dive
              %ul.menu.js-guides
                %li.first.leaf.js-topic
                  %a{:href => "/deep-dive/summerization.html"} Summerization
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/deep-dive/summerization/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/deep-dive/summerization/exercise2.html"} Exercise 2
                %li.leaf.js-topic
                  %a{:href => "/deep-dive/filtering-patterns.html"} Filtering Patterns
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/deep-dive/filtering-patterns/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/deep-dive/filtering-patterns/exercise2.html"} Exercise 2
                %li.leaf.js-topic
                  %a{:href => "/deep-dive/machine-learning.html"} Machine Learning
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/deep-dive/machine-learning/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
                      %a{:href => "/deep-dive/machine-learning/exercise2.html"} Exercise 2
        .body-container.content
          = yield
