%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:property => 'og:type', :content => 'website'}

    %title="Pivotal HD | #{@item[:title]}"
    %link{:href => "/stylesheets/print.css", :media => "print", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/stylesheets/master.css", :media => "screen", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/stylesheets/styles.css", :media => "screen", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/stylesheets/uv_active4d.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/stylesheets/documentation.css", :media => "screen", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "/stylesheets/pygments.css", :media => "screen", :rel => "stylesheet", :type => "text/css"}/
    %script{:src => "/js/jquery-1.9.1.min.js", :type => "text/javascript"}
    %script{:src => "/js/documentation.js", :type => "text/javascript"}
  %body
    .header-wrap
      .container.por
        .logo-wrap
          %a.pngfix.logo{:href => "/"}
        .logo-pivotal
          %a{:style => "color: rgba(22, 21, 21, 0.55);"} Pivotal HD
      .header-shadow
    .main-wrap
      .container
        #sub-nav.nav-container
          %ul.menu
            %li.first.expanded.js-maintopic
              %a{:href => "#"} Introduction
              %ul.menu.js-guides
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/overview.html"} Overview
                %li.leaf.js-subtopic
                  %a{:href => "/introduction/pivotalhd-component.html"} Pivotal HD Components
                %li.leaf.js-topic
                  %a{:href => "/introduction/hdfs.html"} HDFS
                %li.leaf.js-topic
                  %a{:href => "/introduction/yarn.html"} YARN
                %li.leaf.js-topic
                  %a{:href => "/introduction/hive.html"} Hive
                %li.leaf.js-topic
                  %a{:href => "/introduction/pig1.html"} Pig
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/hbase1.html"} Hbase
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/hawq1.html"} HAWQ
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/mahout1.html"} Mahout
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/dataloader1.html"} DataLoader
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/sqoop1.html"} Sqoop
                %li.first.leaf.js-topic
                  %a{:href => "/introduction/hve1.html"} HVE
            %li.expanded.js-maintopic
              %a{:href => "#"} Installation
              %ul.menu.js-guides
                %li.first.leaf.js-topic
                  %a{:href => "/installation/single-node.html"} Single Node
                %li.leaf.js-topic
                  %a{:href => "/installation/multi-node.html"} Multi Node
                %li.leaf.js-topic
                  %a{:href => "/installation/multi-node-vsphere-hve.html"} Multi Node vSphere & HVE

            %li.expanded.active-trail.js-maintopic
              %a.active-trail{:href => "#"} Getting Started
              %ul.menu.js-guides
                %li.first.leaf.js-topic
                  %a{:href => "/getting-started/dataset.html"} Dataset
                %li.leaf.js-topic
                  %a{:href => "/getting-started/map-reduce-java.html"} MapReduce with Java
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/map-reduce-java/mean-calculation-java.html"} Average Energy Consumption
                    %li.leaf.js-subtopic
                      %a{:href => "/getting-started/map-reduce-java/count-businesses-in-city.html"} Count No. of Businesses in a City
                %li.leaf.js-topic
                  %a{:href => "/getting-started/map-reduce-ruby.html"} MapReduce with Ruby
                  %ul.menu.js-subguides
                    %li.first.leaf.js-subtopic
                      %a{:href => "/getting-started/map-reduce-ruby/exercise1.html"} Exercise 1
                    %li.leaf.js-subtopic
